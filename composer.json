{
  "name": "matycz/lemo-validator",
  "description": "Set of data validators for Laminas applications",
  "license": "BSD-3-Clause",
  "type": "library",
  "keywords": [
    "laminas",
    "filter"
  ],
  "homepage": "https://github.com/MatyCZ/lemo-validator",
  "authors": [
    {
      "name": "Matej Szendi"
    }
  ],
  "autoload": {
    "psr-4": {
      "Lemo\\Validator\\": "src/"
    }
  },
  "require": {
    "laminas/laminas-uri": "^2.9",
    "laminas/laminas-validator": "^2.20",
    "php": "^8.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~3.8.0",
    "phpstan/phpstan": "~1.7.0"
  },
  "scripts": {
    "static-analysis": "phpstan analyse -c phpstan.neon"
  }
}
